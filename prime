import java.util.*;

public class Main {
    public static void main(String[] args) {
      System.out.println("Hello, World!");
      int n=10;
      for(int i=2;i<=n;i++)
      {
        if(isPrime(i))
        {
          System.out.println(i+" ");
        }
      }
  }
  public static boolean isPrime(int i)
  {
    if(i==0||i==1) return false;
    for(int k=2;k<i;k++)
    {
      if(i%k==0)
      return false;
    }
    return true;
  }
}
